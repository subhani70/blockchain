# Docker Environment Configuration
# Copy this file to .env and update the values as needed

# Server Configuration
NODE_ENV=production
PORT=5000

# Blockchain Configuration
RPC_URL=http://172.16.10.117:8545
CHAIN_ID=1337
NETWORK_NAME=VoltusWave

# Contract Addresses
# Update this with your deployed registry address
REGISTRY_ADDRESS=0xYourRegistryAddressHere

# Private Key for transactions
# IMPORTANT: Use a dedicated private key for the backend service
# DO NOT use the same key as your main wallet
PRIVATE_KEY=0xYourPrivateKeyHere

# Logging
LOG_LEVEL=info
