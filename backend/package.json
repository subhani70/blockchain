{
  "name": "did-wallet-backend",
  "version": "1.0.0",
  "description": "Decentralized Identity Wallet Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t did-wallet-backend .",
    "docker:run": "docker run -d --name did-wallet-backend -p 5000:5000 did-wallet-backend",
    "docker:start": "./start-docker.sh",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:clean": "docker-compose down && docker rmi did-wallet-backend"
  },
  "keywords": [
    "DID",
    "SSI",
    "blockchain",
    "identity"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "did-jwt": "^7.4.7",
    "did-jwt-vc": "^3.2.10",
    "did-resolver": "^4.1.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "ethr-did": "^2.3.9",
    "ethr-did-resolver": "^8.1.2",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}